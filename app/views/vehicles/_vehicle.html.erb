<% unless vehicle.images.blank? %>
  <%= link_to vehicle_path(vehicle) do %>
    <%= image_tag vehicle.images.first.url , :class=> 'car-image' %>
    <% end %>
  <% end %>

    <div class="label-list">

  <% vehicle.tag_list.map do |t|  %>
    <%= render :partial => 'label' , :locals => {:object => t.to_s} %>
<% end %>
</div>
